#include <iostream>
#include <stdlib.h>
using namespace std;
void findMinMax(const int* arr, int size, int& minVal, int& maxVal) {
    minVal = maxVal = *arr;
    for (int i=0; i<size;i++) {
        if (*(arr+i)>maxVal) {
            maxVal = *(arr+i);
        }
        if (*(arr+i)<minVal) {
            minVal = *(arr+i);
        }
    }
}
int main() {
    int n, min, max;;
    cin>>n;
    int a[n];
    for (int i=0; i<n; i++) {
        cin >>a[i];
    }
    findMinMax(a, n, min, max);
    cout <<"min=" <<min<<endl;
    cout <<"max=" <<max <<endl;
    return 0;
}
