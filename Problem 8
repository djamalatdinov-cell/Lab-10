#include <iostream>
using namespace std;

int cstrLength(char* c) {
    int count = 0;
    while (*c != '\0') {
        count++;
        c++;        
    }
    return count;
}
int main() {
    char str[200];
    cout << "Input: ";
    cin.getline(str, 200);
    cout << "The length is: " << cstrLength(str) << endl;
    return 0;
}
